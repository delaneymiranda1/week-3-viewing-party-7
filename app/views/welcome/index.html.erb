<%= button_to "Create New User", register_path, method: :get %>

<% if current_user %>
    <section class='existing-users'>
        <p>Existing Users: </p>
        <% @users.each do |user| %>
            <li><%= user.email %></li>
        <% end %>
    </section>
<% end %>

<% if cookies[:user_location].present? %>
  <p>Your location: <%= cookies[:user_location] %></p>
<% end %>

<% if current_user %>
  <%= button_to "Log Out", logout_path, method: :delete %>
<% else %>
    <%= link_to "Log In", login_path %>
<% end %>

